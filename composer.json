{
    "name": "xenokore/component-loader",
    "description": "Component loader for the Xeno framework",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Yani",
            "email": "yani@xenokore.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xenokore\\ComponentLoader\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Xenokore\\ComponentLoader\\Tests\\": "tests/",
            "Xenokore\\ComponentLoader\\Tests\\Data\\Author\\Package\\": "tests/data/vendor/author/package/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "escapestudios/symfony2-coding-standard": "^3.12"
    }
}
